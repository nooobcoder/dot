	"editor.quickSuggestionsDelay": 5,
	"window.menuBarVisibility": "visible",
	"diffEditor.codeLens": true,
	"editor.minimap.maxColumn": 200,
	"editor.fontFamily": "'Cascadia Code', 'Fira Code iScript', 'Fira Code', 'JetBrains Mono', 'monospace', 'VictorMono-Regular', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
	"files.autoSaveDelay": 15000,
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.host": "0.0.0.0",
	"liveServer.settings.port": 3000,
	"editor.fontSize": 15,
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"seito-openfile.openNewTab": true,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"python.jediEnabled": false,
	"editor.tabSize": 2,
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.linkedEditing": true,
	"terminal.integrated.tabs.enabled": true,
	"terminal.integrated.tabs.location": "left",
	"editor.fontWeight": "330",
	"terminal.integrated.fontWeight": "360",
	"terminal.integrated.fontFamily": "Cascadia Code",
	"debug.inlineValues": true,
	"C_Cpp.experimentalFeatures": "Enabled",
	"C_Cpp.intelliSenseUpdateDelay": 500,
	"C_Cpp.updateChannel": "Insiders",
	"C_Cpp.vcFormat.indent.caseLabels": true,
	"C_Cpp.vcFormat.indent.braces": true,
	"C_Cpp.vcFormat.indent.accessSpecifiers": true,
	"C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
	"C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
	"C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
	"C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
	"C_Cpp.vcFormat.space.afterCastCloseParenthesis": true,
	"C_Cpp.vcFormat.space.beforeComma": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					//following will be in italic
					"comment",
					"entity.name.type.class", //class names
					"constant", //String, Number, Boolean…, this, super
					"storage.modifier", //static keyword
					"storage.type.class.js", //class keyword
					"keyword" //import, export, return…
				],
				"settings": {
					"fontStyle": "italic"
				}
			},
			{
				"scope": [
					//following will be excluded from italics (VSCode has some defaults for italics)
					"invalid",
					"keyword.operator",
					"constant.numeric.css",
					"keyword.other.unit.px.css",
					"constant.numeric.decimal.js",
					"constant.numeric.json"
				],
				"settings": {
					"fontStyle": ""
				}
			}
		]
	},
	"java.configuration.runtimes": [
		{
			"name": "JavaSE-11",
			"path": "/home/coder/project/javajdk/",
			"default": true
		}
	],
	"java.home": "/home/coder/project/javajdk/",
	"java.semanticHighlighting.enabled": true,
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"workbench.activityBar.visible": true,
	"workbench.sideBar.location": "left",
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"extensions.ignoreRecommendations": true,
	"editor.cursorSmoothCaretAnimation": true,
	"editor.cursorBlinking": "smooth",
	"editor.hover.delay": 100,
	"editor.inlayHints.fontFamily": "'Cascadia Code','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.semanticHighlighting.enabled": true,
	"editor.smoothScrolling": true,
	"editor.tabCompletion": "on",
	"editor.suggest.preview": true,
	"files.eol": "\r\n",
	"workbench.hover.delay": 200,
	"workbench.list.smoothScrolling": true,
	"workbench.welcomePage.experimental.startEntryContributions": true,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"tailwindCSS.emmetCompletions": true,
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"npm-intellisense.packageSubfoldersIntellisense": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode"
}
